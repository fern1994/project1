@model IEnumerable<AntiDrugWeb2.Models.alertmeeting>

@{
    ViewBag.Title = "Index";
}

<h2>Meeting patient</h2>

<p>
    @Html.ActionLink("Create new meeting ", "Create")
</p>
<table class="table table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.meetingDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.meetingTitle)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.meetingDetail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.meetingSummary)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.meetingByStaff)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.meetingDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.meetingTitle)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.meetingDetail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.meetingSummary)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.meetingByStaff)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.alertID }) |
            @Html.ActionLink("Details", "Details", new { id=item.alertID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.alertID })
        </td>
    </tr>
}

</table>
